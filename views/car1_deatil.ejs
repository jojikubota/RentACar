	<script>
	var prevSelCurr='USD';
	function calcExcgRate(){
		var temp1 = document.getElementById("val1").innerHTML;
		var temp2 = document.getElementById("val2").innerHTML;
		var temp3 = document.getElementById("val3").innerHTML;
		var e = document.getElementById("currDrpDwn");
		var selCurr = e.options[e.selectedIndex].value;
		var excgRateAPI = 'http://rate-exchange.herokuapp.com/fetchRate?from=' + prevSelCurr + '&to=' + selCurr;
		var xhttp = new XMLHttpRequest();
		xhttp.onreadystatechange = function() {
			if (this.readyState == 4 && this.status == 200) {
				var obj = JSON.parse(this.responseText);
				document.getElementById("val1").innerHTML = (obj.Rate*temp1).toFixed(2);
				document.getElementById("val2").innerHTML = (obj.Rate*temp2).toFixed(2);
				document.getElementById("val3").innerHTML = (obj.Rate*temp3).toFixed(2);
				document.getElementById("curr1").innerHTML = selCurr;
				document.getElementById("curr2").innerHTML = selCurr;
				document.getElementById("curr3").innerHTML = selCurr;
				prevSelCurr=selCurr;
			}
		};
		xhttp.open("GET", excgRateAPI, true);
		xhttp.send();	
	}
	</script>
